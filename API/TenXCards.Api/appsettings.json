{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "${DB_CONNECTION_STRING}"
  },
  "Jwt": {
    "Key": "${JWT_SECRET}",
    "Issuer": "${JWT_ISSUER}",
    "Audience": "${JWT_AUDIENCE}",
    "TokenValidityInMinutes": "${JWT_TOKEN_VALIDITY_MINUTES}",
    "RefreshTokenValidityInDays": "${JWT_REFRESH_TOKEN_VALIDITY_DAYS}"
  },
  "ClientUrl": "${JWT_AUDIENCE}",
  "AllowedOrigins": "${JWT_AUDIENCE}"
}
