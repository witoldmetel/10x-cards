{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"vcs": {
		"clientKind": "git",
		"defaultBranch": "main",
		"enabled": true,
		"useIgnoreFile": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"suspicious": {
				"noConsoleLog": "warn"
			},
			"correctness": {
				"noUnusedImports": {
					"level": "error",
					"fix": "none"
				}
			}
		}
	},
	"formatter": {
		"enabled": true,
		"formatWithErrors": true
	},
	"css": {
		"linter": {
			"enabled": false
		},
		"formatter": {
			"enabled": false
		},
		"parser": {
			"cssModules": true
		}
	}
}
