{
  "name": "10x-cards-frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc -b && vite build",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "check-updates": "npx npm-check-updates",
    "check-unused-files": "knip",
    "validate": "tsc && npm run lint",
    "prepare": "husky"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.1.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "husky": "^9.1.7",
    "knip": "^5.48.0",
    "typescript": "~5.7.2",
    "vite": "^6.2.0"
  }
}
