{
  "name": "tenxcards",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:e2e": "vite --mode e2e",
    "build": "tsc -b && vite build",
    "check-types": "tsc",
    "lint": "eslint \"./src/**/*.+(js|ts|tsx)\" --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix \"./src/**/*.+(js|ts|tsx)\"",
    "preview": "vite preview",
    "format": "prettier --ignore-path .gitignore --write \"src/**/*.{ts,tsx,json,md}\"",
    "prepare": "husky",
    "validate": "tsc && npm run lint",
    "check-updates": "npx npm-check-updates",
    "check-unused-files": "knip",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "start-server-and-test 'dev:e2e' http://localhost:5173 'playwright test'",
    "test:e2e:ui": "start-server-and-test 'dev:e2e' http://localhost:5173 'playwright test --ui'"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-avatar": "^1.1.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.1.6",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.2.6",
    "@tanstack/react-query": "^5.74.4",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "framer-motion": "^12.9.2",
    "lucide-react": "^0.503.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.1",
    "react-router": "7.5.2",
    "sonner": "^1.4.3",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.2.5",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.11.24",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/ui": "^3.1.3",
    "autoprefixer": "^10.4.21",
    "eslint": "9.24.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "jsdom": "^24.0.0",
    "knip": "^5.50.4",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "start-server-and-test": "^2.0.11",
    "tailwindcss": "3.4.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.31.0",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^3.1.3"
  }
}
