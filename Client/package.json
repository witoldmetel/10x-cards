{
  "name": "tenxcards",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "check-types": "tsc",
    "lint": "eslint \"./src/**/*.+(js|ts|tsx)\" --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix \"./src/**/*.+(js|ts|tsx)\"",
    "preview": "vite preview",
    "format": "prettier --ignore-path .gitignore --write \"src/**/*.{ts,tsx,json,md}\"",
    "prepare": "husky",
    "validate": "tsc && npm run lint",
    "check-updates": "npx npm-check-updates",
    "check-unused-files": "knip"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-avatar": "^1.1.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/react-router-dom": "^5.3.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "lucide-react": "^0.499.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.51.0",
    "react-router": "7.5.1",
    "sonner": "^1.4.3",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.2.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@tailwindcss/vite": "^4.1.4",
    "@types/node": "^20",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "autoprefixer": "^10.4.18",
    "eslint": "9.24.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "knip": "^5.50.4",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.31.0",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
